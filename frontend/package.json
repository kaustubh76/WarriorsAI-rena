{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm run dev:storage\" \"npm run dev:next\"",
    "dev:next": "next dev",
    "dev:storage": "cd 0g-storage && npm run build && npm run start",
    "build": "next build",
    "build:all": "npm run build && cd 0g-storage && npm run build",
    "start": "concurrently \"npm run start:storage\" \"npm run start:next\"",
    "start:next": "next start",
    "start:storage": "cd 0g-storage && npm run start",
    "lint": "next lint"
  },
  "dependencies": {
    "@0glabs/0g-serving-broker": "^0.6.4",
    "@0glabs/0g-ts-sdk": "^0.3.3",
    "@prisma/client": "^5.22.0",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@types/crypto-js": "^4.2.2",
    "@types/react-image-crop": "^8.1.6",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.0.1",
    "ethers": "^6.13.1",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.522.0",
    "next": "^15.3.6",
    "openai": "^5.7.0",
    "pinata": "^2.4.8",
    "pino-pretty": "^13.0.0",
    "prisma": "^5.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.10",
    "viem": "^2.31.4",
    "wagmi": "^2.15.6",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "fake-indexeddb": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
